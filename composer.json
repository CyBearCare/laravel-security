{
    "name": "cybear-care/laravel-security",
    "description": "Enterprise-grade web application security for Laravel applications with centralized management dashboard integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cybear Care",
            "email": "support@cybear.care"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpstan/phpstan": "^1.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "CybearCare\\LaravelSecurity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CybearCare\\LaravelSecurity\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CybearCare\\LaravelSecurity\\CybearSecurityServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}